        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/apex.js&quot;)}

if(&apos;{!DocuSignTemplate__c.Template_ID__c}&apos;!= null) {
    var r = confirm(&apos;{!$Label.fhcm2__DocuSign_Update_Confirm}&apos;);
    if (r == true) {
        var templateID=&apos;{!DocuSignTemplate__c.Template_ID__c}&apos;;
        var status = sforce.apex.execute(&quot;fHCM2.DsServices&quot;,&quot;updateTemplateFromDocuSign&quot;,{templateID: templateID});
        if (status != &apos;OK&apos;) {
            alert(status);
        }
        urlStr = &apos;{!URLFOR(&apos;/apex/fHCM2__DsViewTemplate&apos;)}&apos;;
        location.replace(urlStr+&apos;?id={!DocuSignTemplate__c.Id}&apos;);
    }
}</url>
    </webLinks>
